<script>
// Function to change the content of t2
   function check() {
  const pas = document.getElementById("pass").value;
  const cpas = document.getElementById("cpass").value;
  console.log(pas,cpas);
  if (pas==cpas && pas!="" && cpas!=""){
  document.getElementById("p").innerHTML="Password Matched";
  document.getElementById("p").style.color = "Green";
  }
  else if (pas!=cpas){
  document.getElementById("p").innerHTML="Password does not Match";
  document.getElementById("p").style.color = "red";
  }
 else{
      document.getElementById("p").innerHTML="Password required";
       document.getElementById("p").style.color = "red";
  } 
}
// Add event listener to table with an arrow function
const el = document.getElementById("ad");
el.addEventListener("click", () => { check(); }, false);
</script>


